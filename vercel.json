{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": ".",
  "regions": ["sfo1"],
  "env": {
    "SOLANA_RPC_URL": "https://api.mainnet-beta.solana.com/"
  },
  "routes": [
    {
      "src": "/api/openai",
      "dest": "api/openai.js"
    },
    {
      "src": "/api/webhook",
      "dest": "api/webhook.js"
    },
    {
      "src": "/api/notifications/(.*)",
      "dest": "api/notifications.js?path=$1"
    },
    {
      "handle": "filesystem"
    }
  ]
} 